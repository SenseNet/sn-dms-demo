    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.5",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-fontawesome": "^1.6.3",
    "@types/react-redux": "^6.0.6",
    "@types/react-responsive": "^3.0.1",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-mock-store": "^1.0.0",
    "@types/sinon": "^5.0.1",
    "@types/uuid": "^3.4.3",
    "awesome-typescript-loader": "^5.2.0",
    "codecov": "^3.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-error-overlay": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.4",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.4"
  },
  "scripts": {
    "start": "webpack-dev-server --port 3000 --display-error-details",
    "build": "rimraf build && webpack",
    "test": "node scripts/test.js --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "check-coverage": "npm test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill -- --coverage",
    "lint": "node tslint --project ./tsconfig.json",
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/SensenetStyles.ts",
      "!src/registerServiceWorker.ts",
      "!src/assets/*.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/*.test.ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.ts?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
