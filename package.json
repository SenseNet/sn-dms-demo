{
  "name": "sn-dms-demo",
  "version": "0.1.0-development",
  "description": "Document Management Demo upon sensenet ECM",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SenseNet/sn-dms-demo.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "documentmanagement",
    "MVP",
    "react",
    "reactjs"
  ],
  "author": "SenseNet",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-dms-demo/issues"
  },
  "homepage": "https://sensenet.com",
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@sensenet/authentication-google": "^2.0.0",
    "@sensenet/client-core": "^1.0.2",
    "@sensenet/redux": "^4.0.1",
    "@sensenet/repository-events": "^1.0.0",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.3",
    "autoprefixer": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.3.2",
    "css-loader": "^0.28.10",
    "dotenv": "5.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.0.1",
    "material-design-icons": "^3.0.1",
    "material-ui": "^1.0.0-beta.35",
    "material-ui-icons": "^1.0.0-beta.15",
    "moment": "^2.19.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "^2.1.1",
    "promise": "8.0.1",
    "react": "^16.0.0",
    "react-async-script": "^0.9.1",
    "react-dev-utils": "^5.0.0",
    "react-dnd": "^2.5.3",
    "react-dnd-html5-backend": "^2.5.3",
    "react-dom": "^16.0.0",
    "react-fontawesome": "^1.6.1",
    "react-google-recaptcha": "^0.9.7",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.6",
    "react-responsive": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "3.0.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.20.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^22.4.2",
    "ts-keycode-enum": "^1.0.4",
    "ts-loader": "^3.5.0",
    "typeface-roboto": "0.0.54",
    "url-loader": "1.0.1",
    "webpack-dev-server": "2.11.1",
    "webpack-manifest-plugin": "2.0.0-rc.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.0",
    "codecov.io": "^0.1.6",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "react-error-overlay": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "sinon": "^4.4.6",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.7.2",
    "webpack": "4.2.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "check-coverage": "npm test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill -- --coverage",
    "lint": "node tslint --project ./tsconfig.json",
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/SensenetStyles.ts",
      "!src/registerServiceWorker.ts",
      "!src/assets/*.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
