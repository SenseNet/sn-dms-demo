{
  "name": "sn-dms-demo",
  "version": "0.1.0-development",
  "description": "Document Management Demo upon sensenet ECM",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SenseNet/sn-dms-demo.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "documentmanagement",
    "MVP",
    "react",
    "reactjs"
  ],
  "author": "SenseNet",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-dms-demo/issues"
  },
  "homepage": "https://sensenet.com",
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/jest": "^21.1.0",
    "@types/node": "^8.0.24",
    "@types/react": "^16.0.3",
    "@types/react-dom": "^15.5.4",
    "app-root-path": "^2.0.1",
    "autoprefixer": "7.1.4",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.1.0",
    "cli-highlight": "1.1.4",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "4.0.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.0",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "material-ui-icons": "^1.0.0-beta.10",
    "moment": "^2.18.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-async-script": "^0.9.1",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^15.6.1",
    "react-error-overlay": "1.0.8",
    "react-google-recaptcha": "^0.9.7",
    "react-moment": "^0.6.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "2.0.1",
    "sensenet-kfi-cz-conventional-changelog": "^1.0.0",
    "sn-client-js": "^2.4.1",
    "sn-redux": "^3.2.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^21.0.0",
    "ts-loader": "^2.2.1",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typeface-roboto": "0.0.35",
    "typescript": "~2.5.2",
    "url-loader": "0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "material-ui": "^1.0.0-beta.11",
    "webpack": "^3.5.5"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "node tslint --project ./tsconfig.json",
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
